{
  "name": "coffee-lex",
  "description": "Stupid lexer for CoffeeScript.",
  "main": "dist/coffee-lex.js",
  "jsnext:main": "dist/coffee-lex.mjs",
  "scripts": {
    "prebuild": "rm -rf dist/",
    "build": "rollup -c && cp src/coffee-lex.js.flow dist/",
    "flow": "flow check --all",
    "pretest": "npm run flow",
    "test": "mocha",
    "prepublish": "npm run flow && npm run build",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/decaffeinate/coffee-lex.git"
  },
  "keywords": [
    "coffee",
    "coffeescript",
    "lex",
    "parse"
  ],
  "files": [
    "dist"
  ],
  "author": "Brian Donovan <me@brian-donovan.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/decaffeinate/coffee-lex/issues"
  },
  "homepage": "https://github.com/decaffeinate/coffee-lex",
  "devDependencies": {
    "babel-plugin-syntax-class-properties": "^6.5.0",
    "babel-plugin-syntax-flow": "^6.5.0",
    "babel-plugin-transform-class-properties": "^6.6.0",
    "babel-plugin-transform-flow-strip-types": "^6.7.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-es2015-rollup": "^1.1.1",
    "babel-register": "^6.7.2",
    "babelrc-rollup": "^1.1.0",
    "cz-conventional-changelog": "^1.1.6",
    "flow-bin": "^0.26.0",
    "mocha": "^2.4.5",
    "rollup": "^0.33.0",
    "rollup-plugin-babel": "^2.4.0",
    "semantic-release": "^4.3.5"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
