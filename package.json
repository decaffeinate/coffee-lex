{
  "name": "coffee-lex",
  "description": "Stupid lexer for CoffeeScript.",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "check-format": "prettier-check '{src,test}/**/*.ts' *.md",
    "reformat": "prettier --write '{src,test}/**/*.ts' *.md",
    "prelint": "yarn check-format",
    "lint": "tslint --config tslint.json --project tsconfig.json",
    "prelint-fix": "yarn reformat",
    "lint-fix": "tslint --config tslint.json --project tsconfig.json --fix",
    "pretest": "yarn lint",
    "test": "mocha"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/decaffeinate/coffee-lex.git"
  },
  "keywords": [
    "coffee",
    "coffeescript",
    "lex",
    "parse"
  ],
  "files": [
    "dist"
  ],
  "author": "Brian Donovan",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/decaffeinate/coffee-lex/issues"
  },
  "homepage": "https://github.com/decaffeinate/coffee-lex",
  "devDependencies": {
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.12.10",
    "decaffeinate-coffeescript": "1.12.7-patch.2",
    "mocha": "^5.2.0",
    "prettier": "^1.15.2",
    "prettier-check": "2.0.0",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "typescript": "^3.1.6"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  }
}
