{
  "name": "coffee-lex",
  "description": "Stupid lexer for CoffeeScript.",
  "main": "dist/coffee-lex.js",
  "jsnext:main": "dist/coffee-lex.mjs",
  "scripts": {
    "prebuild": "rm -rf dist/",
    "build": "rollup -c && cp src/coffee-lex.js.flow dist/",
    "typecheck": "tsc -p .",
    "pretest": "npm run typecheck",
    "test": "mocha",
    "prepublish": "npm run typecheck && npm run build",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/decaffeinate/coffee-lex.git"
  },
  "keywords": [
    "coffee",
    "coffeescript",
    "lex",
    "parse"
  ],
  "files": [
    "dist"
  ],
  "author": "Brian Donovan <me@brian-donovan.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/decaffeinate/coffee-lex/issues"
  },
  "homepage": "https://github.com/decaffeinate/coffee-lex",
  "devDependencies": {
    "@types/node": "^6.0.51",
    "cz-conventional-changelog": "^1.1.6",
    "decaffeinate-coffeescript": "^1.10.0-patch5",
    "mocha": "^3.0.0",
    "mocha-typescript": "^1.0.11",
    "reify": "^0.4.4",
    "rollup": "^0.36.0",
    "rollup-plugin-typescript": "^0.8.1",
    "semantic-release": "^6.3.2",
    "typescript": "^2.0.10"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
